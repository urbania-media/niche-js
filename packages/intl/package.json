    "main": "lib/index.js",
    "module": "es/index.js",
    "exports": {
        ".": {
            "require": "./lib/index.js",
            "import": "./es/index.js"
        },
        "./locale/*.json": "./locale/*.json",
        "./locale/*": {
            "require": "./locale/*.cjs.js",
            "import": "./locale/*.js"
        }
    },
    "files": [
        "locale",
        "lib",
        "es",
        "scripts",
        "assets"
    ],
    "scripts": {
        "intl": "./scripts/build-intl.sh",
        "extract": "./scripts/extract.js '../../!(node_modules)/!(intl)/src/**/*.+(js|jsx)' ./lang/messages.json",
        "prepare": "rm -rf ./locale && npm run intl && ../../scripts/prepare-package.sh"
    },
    "devDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.21.0",
        "@formatjs/cli-lib": "^6.0.3",
