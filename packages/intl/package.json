        "commander": "^10.0.0",
        "fs-extra": "^11.1.0",
        "gettext-parser": "^4.0.4",
        "glob": "^9.3.0",
        "lodash": "^4.17.21",
        "prop-types": "^15.8.1",
        "react-intl": "^6.3.0"
    },
    "lang": "locale",
    "supportedLocales": [
        "en",
        "fr"
