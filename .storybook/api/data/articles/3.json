                        "height": 376
                    },
                    {
                        "id": "medium",
                        "url": "https://v2.urbania.ca/storage/image/2023-08-03/23376-085347-filters(medium).png",
                        "width": 800,
                        "height": 601
                    },
                    {
                        "id": "large",
                        "url": "https://v2.urbania.ca/storage/image/2023-08-03/23376-085347-filters(large).png",
                        "width": 1238,
                        "height": 930
                    }
                ]
