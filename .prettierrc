{
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": true,
    "importOrder": [
        "^(fs|path)$",
        "<THIRD_PARTY_MODULES>",
        "^@niche-js/(.*)$",
        "^[./](.*)?/(utils|hooks|lib)",
        "^[./].*(?<!(\\.[a-z]+))$",
        "\\.scss$",
        "\\.[a-z]+$"
    ],
    "importOrderSeparation": true
}
